<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE web-app
     PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
     "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
     
<web-app>
  <display-name> CIS555 HTTP Serverlet </display-name>

  <servlet>
    <servlet-name>555HTTPServlet</servlet-name>
    <servlet-class> edu.upenn.cis.cis455.webserver.servlet.myHttpServlet </servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>555HTTPServlet</servlet-name>
    <url-pattern>/servlet</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>default</servlet-name>
    <url-pattern>*.html</url-pattern>
  </servlet-mapping>


</web-app>