<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE web-app
     PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
     "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
     
<web-app>
  <display-name> CIS555 HTTP Serverlet </display-name>

  <servlet>
    <servlet-name>testServlet</servlet-name>
    <servlet-class>edu.upenn.cis.cis455.webserver.servlet.TestServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>loginServlet</servlet-name>
    <servlet-class>edu.upenn.cis.cis455.webserver.servlet.LoginServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>calculatorServlet</servlet-name>
    <servlet-class>edu.upenn.cis.cis455.webserver.servlet.CalculatorServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>GetCookie</servlet-name>
    <servlet-class>edu.upenn.cis.cis455.webserver.servlet.GetCookie</servlet-class>
  </servlet>
  
   <servlet>
    <servlet-name>SetCookie</servlet-name>
    <servlet-class>edu.upenn.cis.cis455.webserver.servlet.SetCookie</servlet-class>
  </servlet>
  
  
   <servlet-mapping>
    <servlet-name>testServlet</servlet-name>
    <url-pattern>/calculate/banana/</url-pattern>
   </servlet-mapping>
   
   
   <servlet-mapping>
    <servlet-name>loginServlet</servlet-name>
    <url-pattern>/login/</url-pattern>
   </servlet-mapping>
   
   <servlet-mapping>
    <servlet-name>calculatorServlet</servlet-name>
    <url-pattern>/calculate/*</url-pattern>
   </servlet-mapping>
   
   <servlet-mapping>
    <servlet-name>GetCookie</servlet-name>
    <url-pattern>/GetCookie/</url-pattern>
   </servlet-mapping>
   
   <servlet-mapping>
    <servlet-name>SetCookie</servlet-name>
    <url-pattern>/SetCookie/</url-pattern>
   </servlet-mapping>
  

</web-app>